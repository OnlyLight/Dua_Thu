import java.util.Scanner;

public class text {

	public static void main(String[] args) {
		
		int diem;
		System.out.println("Nhap vao diem: ");
		Scanner a = new Scanner(System.in);
		diem = a.nextInt();
		
		if(diem >= 8 && diem <= 10)
		{
			System.out.println("HE so A");
		}
		else if(diem >= 7 && diem < 8)
		{
			System.out.println("he so B");
		}
		else if(diem >= 6 && diem < 7)
		{
			System.out.println("he so C");
		}
		else if(diem >= 5 && diem < 6)
		{
			System.out.println("he so D");
		}
		else
		{
			System.out.println("he so E");
		}
		
		
	}

}
